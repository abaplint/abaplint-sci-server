{
  "name": "abaplint-sci-server",
  "version": "1.0.0",
  "private": true,
  "description": "testing",
  "main": "build/index.js",
  "bin": {
    "abaplint-backend": "build/index.js"
  },
  "scripts": {
    "run": "ts-node index.ts",
    "lint": "eslint index.ts src/**/*.ts",
    "build": "tsc && cp -R public ./build",
