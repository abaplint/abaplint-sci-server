  "bugs": {
    "url": "https://github.com/abaplint/abaplint-sci-server/issues"
  },
  "engines": {
    "node": "16.x.x"
  },
  "homepage": "https://github.com/abaplint/abaplint-sci-server#readme",
  "dependencies": {
    "@abaplint/core": "^2.113.106",
    "commander": "^8.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/helmet": "4.0.0",
    "@types/jest": "^29.5.13",
